  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "background": "#F5F2EE",
        "foreground": "#657B83"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6D686E"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#415603"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#B8424E"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#89641A"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable.language", "variable.other"],
      "settings": {
        "foreground": "#622272"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#4a5e02",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#073642"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#0C569B"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#0657A3"
      }
    },
    {
      "name": "Variable start",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#4a5e02"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#B8424E"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "meta.preprocessor"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#89641A"
      }
    },
    {
      "name": "Support.construct",
      "scope": ["support.function.construct", "keyword.other.new"],
      "settings": {
        "foreground": "#B8424E"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#622272"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#6D686E"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6D686E"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#622272"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#B8424E"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#622272",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    },
    {
      "name": "diff: header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#622272"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "fontStyle": "",
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "fontStyle": "",
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#4a5e02"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#89641A"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#89641A"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#8E2A34"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#622272"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#502C59"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#316bcd"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#cd3131"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#800080"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
